<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE library PUBLIC "-//NetBeans//DTD Library Declaration 1.0//EN" "http://www.netbeans.org/dtds/library-declaration-1_0.dtd">
<library version="1.0">
    <name>SQL Server Driver</name>
    <type>j2se</type>
    <localizing-bundle>com/codealchemia/netbeans/sqlserver/driver/Bundle</localizing-bundle>
    <volume>
        <type>classpath</type>
        <!-- see http://bits.netbeans.org/8.1/javadoc/org-openide-modules/org/openide/modules/InstalledFileLocator.html for more info about nbinst -->
        <!-- see also http://wiki.netbeans.org/DevFaqUriVsUrl -->
        <!-- net.java.dev.koalalayout.nbsupport is the module name. nbinst uses this entry for locate the path to module's cluster. 
        The remaining of the URL is the path of the JAR file from within the cluster dir. It must be the same as the "Class-Path" entry
        in the module MANIFEST file. The latter is generated by te NBM maven plugin. So make sure they match -->
        <resource>jar:nbinst:///modules/ext/${project.groupId}.${project.artifactId}/${jdbc.groupId}/${jdbc.artifactId}.jar!/</resource>
    </volume>
</library>
